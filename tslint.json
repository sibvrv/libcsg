{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-no-circular-imports"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "no-unused-expression": false,
    "no-var-requires": false,
    "no-bitwise": false,
    "prefer-for-of": false
  }
}
